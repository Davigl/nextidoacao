<div class="row">
  <div class="col  l4 m4  s12 offset-l3 offset-m3"><h1 class="center"> Endereços</h1></div>
  <div class="col l3 offset-s4"><a class=" btn  btn-large waves-effect waves-ligh azul botao-baixo" href="enderecos/new"><i class="material-icons">add</i></a></div>
</div>

<div class="container">
<p id="notice"><%= notice %></p>
<table class="centered striped responsive-table flow-text">
  <thead>
    <tr>
      <th>Logradouro</th>
      <th>Tipo logradouro</th>
      <th>Número</th>
      <th>Bairro</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @enderecos.each do |endereco| %>
      <tr>
        <td><%= endereco.logradouro %></td>
        <td><%= endereco.tipo_logradouro %></td>
        <td><%= endereco.numero %></td>
        <td><%= endereco.bairro %></td>
        <td><%= endereco.cidade %></td>
        <td><%= endereco.estado %></td>
        <td><%= link_to 'Show', endereco %></td>
        <td><%= link_to 'Edit', edit_endereco_path(endereco) %></td>
        <td><%= link_to 'Destroy', endereco, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>


</div>